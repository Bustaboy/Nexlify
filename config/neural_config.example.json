{
  "exchanges": {},
  "btc_wallet_address": "YOUR_WALLET_ADDRESS_HERE",
  "min_withdrawal": 100,
  "auto_trade": false,
  "enable_phase1_phase2_integration": true,

  "risk_management": {
    "enabled": true,
    "max_position_size": 0.05,
    "max_daily_loss": 0.05,
    "stop_loss_percent": 0.02,
    "take_profit_percent": 0.05,
    "use_kelly_criterion": true,
    "kelly_fraction": 0.5,
    "min_kelly_confidence": 0.6,
    "max_concurrent_trades": 3
  },

  "circuit_breaker": {
    "enabled": true,
    "failure_threshold": 3,
    "timeout_seconds": 300,
    "half_open_max_calls": 1
  },

  "performance_tracking": {
    "enabled": true,
    "database_path": "data/trading.db",
    "calculate_sharpe_ratio": true,
    "risk_free_rate": 0.02,
    "track_drawdown": true,
    "export_formats": ["json", "csv"]
  },

  "environment": {
    "debug": false,
    "log_level": "INFO",
    "api_port": 8000,
    "database_url": "sqlite:///data/trading.db",
    "emergency_contact": "YOUR_EMAIL_OR_PHONE",
    "telegram_bot_token": "YOUR_TELEGRAM_BOT_TOKEN",
    "telegram_chat_id": "YOUR_TELEGRAM_CHAT_ID"
  },

  "security": {
    "pin": "YOUR_4_TO_8_DIGIT_PIN",
    "default_user": "nexlify_user"
  },

  "pin_authentication": {
    "enabled": true,
    "min_length": 4,
    "max_length": 8,
    "allow_sequential": false,
    "allow_repeated": false,
    "max_failed_attempts": 3,
    "lockout_duration_minutes": 15,
    "session_timeout_minutes": 30,
    "require_pin_change_days": 90
  },

  "emergency_kill_switch": {
    "enabled": true,
    "auto_backup": true,
    "close_positions": true,
    "cancel_orders": true,
    "lock_on_trigger": true,
    "telegram_notify": true,
    "require_pin_unlock": true,
    "flash_crash_threshold": 0.15,
    "api_failure_threshold": 5
  },

  "flash_crash_protection": {
    "enabled": true,
    "minor_threshold": -0.05,
    "major_threshold": -0.10,
    "critical_threshold": -0.15,
    "check_interval": 30,
    "timeframes": ["1m", "5m", "15m"],
    "recovery_threshold": 0.20,
    "volume_spike_threshold": 3.0
  },

  "integrity_monitor": {
    "enabled": true,
    "critical_files": [
      "config/neural_config.json",
      "nexlify_risk_manager.py",
      "nexlify_advanced_security.py",
      "nexlify_emergency_kill_switch.py",
      "nexlify_flash_crash_protection.py",
      "nexlify_pin_manager.py",
      "nexlify_integrity_monitor.py",
      "nexlify_neural_net.py",
      "cyber_gui.py"
    ],
    "check_interval": 300,
    "auto_baseline_update": false,
    "trigger_killswitch_on_critical": true,
    "allowed_processes": ["python", "python3", "nexlify", "ccxt"]
  },

  "tax_reporting": {
    "enabled": true,
    "jurisdiction": "us",
    "cost_basis_method": "fifo",
    "long_term_threshold_days": 365,
    "database_path": "data/trading.db",
    "auto_generate_reports": true,
    "report_frequency": "yearly"
  },

  "defi_integration": {
    "enabled": true,
    "idle_threshold": 1000,
    "min_apy": 5.0,
    "max_risk": "medium",
    "auto_compound": true,
    "compound_threshold": 50,
    "networks": ["ethereum", "polygon"],
    "protocols": {
      "uniswap_v3": {
        "enabled": true,
        "min_apy": 5.0,
        "max_risk": "medium"
      },
      "aave": {
        "enabled": true,
        "min_apy": 3.0,
        "max_risk": "low"
      },
      "pancakeswap": {
        "enabled": false,
        "min_apy": 8.0,
        "max_risk": "high"
      }
    }
  },

  "profit_management": {
    "enabled": true,
    "min_operating_balance": 1000,
    "default_strategy": "threshold",
    "default_percentage": 50,
    "default_threshold": 1000,
    "auto_compound": true,
    "compound_percentage": 50,
    "database_path": "data/trading.db",
    "withdrawal_destinations": {
      "cold_wallet": {
        "enabled": true,
        "address": "YOUR_COLD_WALLET_ADDRESS"
      },
      "bank_account": {
        "enabled": false,
        "details": "YOUR_BANK_DETAILS"
      }
    }
  },

  "trading": {
    "enabled": true,
    "min_profit_percent": 0.5,
    "max_position_size": 100,
    "max_concurrent_trades": 5,
    "max_daily_loss": 100,
    "take_profit": 5.0,
    "stop_loss": 2.0,
    "trailing_stop": 3.0,
    "max_hold_time_hours": 24,
    "min_confidence": 0.7
  },

  "rl_agent": {
    "discount_factor": 0.99,
    "epsilon_start": 1.0,
    "epsilon_min": 0.01,
    "epsilon_decay": 0.995,
    "learning_rate": 0.001,
    "replay_buffer_size": 100000,
    "batch_size": 64,
    "target_update_frequency": 10,
    "save_frequency": 100,
    "architectures": {
      "tiny": [64, 32],
      "small": [128, 64],
      "medium": [128, 128, 64],
      "large": [256, 256, 128, 64],
      "xlarge": [512, 512, 256, 128, 64]
    },
    "default_architecture": "medium",
    "timeframe": "1h",
    "manual_gamma": null,
    "gradient_clip_norm": 1.0,
    "gradient_explosion_threshold": 10.0,
    "gradient_vanishing_threshold": 1e-06,
    "lr_scheduler_type": "cosine",
    "lr_warmup_enabled": true,
    "lr_warmup_steps": 1000,
    "lr_warmup_start": 1e-05,
    "lr_min": 1e-06,
    "lr_cosine_T_0": 50,
    "lr_cosine_T_mult": 2,
    "lr_plateau_factor": 0.5,
    "lr_plateau_patience": 10,
    "lr_step_size": 500,
    "lr_step_gamma": 0.5,
    "use_double_dqn": true,
    "use_dueling_dqn": true,
    "dueling_shared_sizes": [128, 128],
    "dueling_value_sizes": [64],
    "dueling_advantage_sizes": [64],
    "dueling_aggregation": "mean",
    "track_q_values": true
  },

  "epsilon_decay": {
    "type": "scheduled",
    "start": 1.0,
    "end": 0.22,
    "linear_decay_steps": 2000,
    "scheduled_milestones": {
      "0": 1.0,
      "200": 0.65,
      "800": 0.35,
      "2000": 0.22
    }
  },

  "ensemble": {
    "enabled": false,
    "strategy": "weighted_avg",
    "ensemble_size": 3,
    "diversity_penalty": 0.1,
    "high_uncertainty_threshold": 0.5,
    "meta_model_hidden_size": 64,
    "meta_learning_rate": 0.001,
    "training": {
      "num_models": 5,
      "episodes_per_model": 1000,
      "parallel_training": true,
      "max_workers": null,
      "seed_start": 42,
      "epsilon_variations": true,
      "save_all_checkpoints": true,
      "validation_episodes": 100,
      "output_dir": "./models/ensemble"
    },
    "model_selection": {
      "selection_method": "diverse",
      "min_validation_score": 0.0,
      "max_correlation": 0.8,
      "use_diversity_penalty": true
    },
    "inference": {
      "return_uncertainty": true,
      "log_high_uncertainty": true,
      "log_disagreements": true,
      "uncertainty_action": "warn"
    }
  },

  "paths": {
    "data_dir": "data",
    "logs_dir": "logs",
    "cache_dir": "cache",
    "models_dir": "models",
    "reports_dir": "reports",
    "trading_database": "data/trading.db",
    "audit_logs_dir": "logs/audit",
    "crash_reports_dir": "logs/crash_reports",
    "defi_positions_file": "data/defi_positions.json",
    "flash_crash_events_log": "data/flash_crash_events.jsonl",
    "risk_state_file": "data/risk_state.json"
  },

  "backtesting": {
    "enabled": true,
    "initial_balance": 10000,
    "start_date": "2023-01-01",
    "end_date": "2023-12-31",
    "symbols": ["BTC/USDT", "ETH/USDT"],
    "timeframe": "1h",
    "commission": 0.001
  },

  "websocket_feeds": {
    "enabled": true,
    "exchanges": ["binance", "kraken"],
    "symbols": ["BTC/USDT", "ETH/USDT"],
    "reconnect_delay": 5,
    "max_reconnect_attempts": 10
  },

  "multi_timeframe": {
    "enabled": true,
    "timeframes": ["5m", "15m", "1h", "4h", "1d"],
    "primary_timeframe": "1h",
    "confirmation_timeframes": ["4h", "1d"]
  },

  "paper_trading": {
    "enabled": false,
    "initial_balance": 10000,
    "track_performance": true,
    "simulate_fees": true,
    "fee_percent": 0.1,
    "slippage_percent": 0.05
  },

  "portfolio_rebalancing": {
    "enabled": false,
    "frequency": "weekly",
    "target_allocation": {
      "BTC": 0.4,
      "ETH": 0.3,
      "stable": 0.3
    },
    "rebalance_threshold": 0.05,
    "min_rebalance_amount": 100
  },

  "auto_trader": {
    "enabled": false,
    "check_interval_seconds": 60,
    "exit_strategy": {
      "take_profit_percent": 0.05,
      "stop_loss_percent": 0.02,
      "trailing_stop_percent": 0.03
    }
  },

  "gui": {
    "cpu_sample_interval_seconds": 1,
    "http_timeout_seconds": 5,
    "default_scan_interval_seconds": 300,
    "chart_update_interval_ms": 5000,
    "intense_mode": {
      "scan_interval_seconds": 60,
      "chart_update_interval_ms": 1000
    },
    "balanced_mode": {
      "scan_interval_seconds": 120,
      "chart_update_interval_ms": 3000
    },
    "conservative_mode": {
      "scan_interval_seconds": 300,
      "chart_update_interval_ms": 5000
    }
  },

  "monitoring": {
    "sample_interval_seconds": 0.1,
    "resize_check_interval_seconds": 60,
    "thread_join_timeout_seconds": 1.0,
    "neural_net_update_interval_seconds": 60
  },

  "training_dashboard": {
    "enabled": true,
    "port": 8050,
    "update_interval_ms": 2000,
    "theme": "cyberpunk",
    "auto_start": false,
    "metrics_logging": {
      "enabled": true,
      "output_dir": "training_logs",
      "buffer_size": 100,
      "auto_save_interval": 50,
      "enable_async": true,
      "save_formats": ["json", "csv"]
    },
    "alerts": {
      "enabled": true,
      "email": {
        "enabled": false,
        "smtp_host": "smtp.gmail.com",
        "smtp_port": 587,
        "username": "your@email.com",
        "password": "your_password",
        "from_addr": "nexlify@bot.com",
        "to_addrs": ["recipient@email.com"]
      },
      "slack": {
        "enabled": false,
        "webhook_url": "https://hooks.slack.com/services/YOUR/WEBHOOK/URL"
      },
      "thresholds": {
        "no_improvement_episodes": 100,
        "critical_loss_threshold": 1000.0,
        "min_profit_warning": -500.0
      },
      "throttle_window_seconds": 300
    },
    "experiment_tracking": {
      "enabled": true,
      "experiments_dir": "experiments",
      "auto_save": true,
      "track_hyperparameters": true,
      "leaderboard_size": 20
    },
    "diagnostics": {
      "track_q_values": true,
      "track_gradients": true,
      "track_weights": true,
      "track_activations": false,
      "gradient_logging_interval": 10,
      "weight_logging_interval": 50
